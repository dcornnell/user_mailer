


%br
.row
	.small-4.large-4.columns.small-centered
		=form_tag sessions_path do
			%ul.pricing-table
				%li.title Login
				%li.bullet-item Email
				%li
					= text_field_tag :email
				%li.bullet-item Password
				%li
					= password_field_tag :password
				%li.cta-button
					= submit_tag "Login", class: "button"
.row
	.small-4.large-4.columns.small-centered
		.loginClicker.button.expand
			Login
		.div{id:"login"}
			=form_tag sessions_path do
				%ul.pricing-table
					%li.title Login
					%li.bullet-item Email
					%li
						= text_field_tag :email
					%li.bullet-item Password
					%li
						= password_field_tag :password
					%li.cta-button
						= submit_tag "Login", class: "button"
			.xOut.button.expand
				close



.row
	
	.small-4.large-4.columns.small-centered
		=link_to "Create New User", new_user_path, class: "button expand"
.row
	.small-6.large-6.columns.small-centered
		%h1 List of all the users
		%br
		%b DONT Click on them it deletes them for ever!
	%br
	%ul{class: "small-block-grid-4"}
		
		- @users.each do |u|
			%li
				
				.panel{"data-user-id" => u.id}
					

					=u.name
					
					
			


					







		